import java.util.*;
public class Coda {
	private Vector elementi;
	public coda() {
		elementi = new Vector();
	}
	public void aggiungi(Object obj)
	{
		elementi.addElement(obj);
	}
	public void togli()
	{
		Object obj=null;
		int size=elementi.size();
		
		if(size>0)
		{
			obj=elementi.elementAt(0);
			elementi.removeElementAt(0);
		}
		return obj;
	}
	
}
